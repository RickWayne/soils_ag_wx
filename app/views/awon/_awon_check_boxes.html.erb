<table border="1">
  <tr>
  <% last = 0; check_boxes(@ahrs) do |check_tag,label_tag,ii| %>
    <td><%= check_tag %>  <%= label_tag %></td>
    <% if (ii+1) % 3 == 0 %>
      </tr><tr>
    <% end %>
    <% last = ii %>
  <% end %>
  <% (3 - (last % 3)).times do %>
    <td/>
  <% end %>
  <tr/>
</table>
